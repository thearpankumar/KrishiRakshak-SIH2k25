# n8n Configuration
# Copy this file to .env and modify the values as needed

# Basic Authentication Credentials (CHANGE THESE!)
N8N_BASIC_AUTH_USER=example 
N8N_BASIC_AUTH_PASSWORD=changeme123

# Database Configuration
DB_POSTGRES_USER=n8n
DB_POSTGRES_PASSWORD=n8npassword
DB_POSTGRES_DB=n8n

# Redis Configuration (optional)
REDIS_PASSWORD=redispassword

# Network Configuration
# Replace localhost with your server's IP for network access
# Example: WEBHOOK_URL=http://192.168.1.100:5678/
WEBHOOK_URL=http://localhost:5678/

# Timezone Configuration
GENERIC_TIMEZONE=Aisa/Kolkata
TZ=Asia/Kolkata 

# Server IP for network access (update this with your server's local IP)
# Example: SERVER_IP=192.168.1.100
SERVER_IP=localhost
